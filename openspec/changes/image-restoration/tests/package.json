{
  "name": "image-restoration-spec-tests",
  "version": "1.0.0",
  "description": "Validation tests for Image Restoration OpenSpec Proposal",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ],
    "collectCoverageFrom": [
      "*.test.js"
    ]
  }
}